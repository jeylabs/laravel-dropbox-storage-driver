{
    "name": "jeylabs/laravel-dropbox-storage-driver",
    "description": "Dropbox storage driver for Laravel.",
    "homepage": "https://github.com/jeylabs/laravel-dropbox-storage-driver",
    "license": "MIT",
    "authors": [
        {
            "name": "jeylabs",
            "email": "ratheep.ceymplon@jeylabs.com.au",
            "homepage": "https://jeylabs.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/jeylabs/laravel-dropbox-storage-driver/issues"
    },
    "keywords": [
        "laravel",
        "driver",
        "storage",
        "dropbox"
    ],
    "autoload": {
        "psr-4": {
            "Jeylabs\\Laravel\\DropboxDriver\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "^5.0",
        "league/flysystem-dropbox": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "orchestra/testbench": "^3.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
